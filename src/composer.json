{
    "name": "cora/cora",
    "description": "Demo project for the Cora development framework. Cora is a framework for easily building MVC style web applications.",
    "keywords": ["framework", "cora", "demo"],
    "license": "MIT",
    "authors": [
        {
            "name": "Josiah Bubna"
        },
        {
            "name": "Aaron Rabideau"
        }
    ],
    "config": {
        "platform": {
            "php": "8.2.19"
        },
        "sort-packages": true
    },
    "require": {
        "php": ">=5.3.0",
        "cora/cora-framework": "*",
        "cora/autoload": "*",
        "phpunit/phpunit": "11.*"
    },
    "autoload": {
      "psr-4": {
        "Cora\\": ["vendor/cora/autoload/app/", "vendor/cora/cora-framework/system/classes/"]
      }
    }
}